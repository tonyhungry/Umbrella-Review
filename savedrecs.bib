@article{ WOS:000189184500020,
Author = {Ghyselinck, M and Custers, R and Brysbaert, M},
Title = {The effect of age of acquisition in visual word processing: Further
   evidence for the semantic hypothesis},
Journal = {JOURNAL OF EXPERIMENTAL PSYCHOLOGY-LEARNING MEMORY AND COGNITION},
Year = {2004},
Volume = {30},
Number = {2},
Pages = {550-554},
Month = {MAR},
Abstract = {The authors investigated whether the meaning of visually presented words
   is activated faster for early-acquired words than for late-acquired
   words. They addressed the issue using the semantic Simon paradigm. In
   this paradigm, participants are instructed to decide whether a stimulus
   word is printed in uppercase or lowercase letters. However, they have to
   respond with a verbal label ({''}living{''} or ``nonliving{''}) that is
   either congruent with the meaning of the word (e.g., saying ``living{''}
   to the stimulus DOG) or incongruent (e.g., saying ``nonliving{''} to the
   stimulus dog). Results showed a significant congruency effect that was
   stronger for early-acquired words than for late-acquired words. The
   authors conclude that the age of acquisition is an important variable in
   the activation of the meaning of visually presented words.},
