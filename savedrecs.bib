@article{ WOS:000510827100006,
Author = {Bors, Christian and Wenskovitch, John and Dowling, Michelle and
   Attfield, Simon and Battle, Leilani and Endert, Alex and Kulyk, Olga and
   Laramee, Robert S.},
Title = {A Provenance Task Abstraction Framework},
