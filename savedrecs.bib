@article{ WOS:000237827700002,
Author = {Ashford, Derek and Bennett, Simon J. and Davids, Keith},
Title = {Observational modeling effects for movement dynamics and movement
   outcome measures across differing task constraints: A meta-analysis},
Journal = {JOURNAL OF MOTOR BEHAVIOR},
Year = {2006},
Volume = {38},
Number = {3},
Pages = {185-205},
Month = {MAY},
Abstract = {The authors conducted a meta-analysis of the observational modeling
   literature to quantify overall between-participants treatment effects
   obtained when movement behaviors are acquired. Effects were obtained and
   reported separately for movement dynamics (MD) and movement outcome (MO)
   measures. The overall mean observational modeling treatment effects
   (delta{''}(Bi)) were 0.77 and 0.17. respectively. for MD and MO
   measures. The effects reflected a significant advantage of observational
   modeling over practice-only control conditions. Most important, the
   magnitude of the obtained effects was far stronger for MD than for MO
   measures, confirming a distinctive response to observational modeling
   during motor learning. The advantage for MD measures over observational
