@inproceedings{ WOS:000273285200004,
Author = {Nielsen, Soren Holbech and Parsons, Simon},
Editor = {Dunne, PE and BenchCapon, TJM},
Title = {An Application of Formal Argumentation: Fusing Bayes Nets in MAS},
Booktitle = {COMPUTATIONAL MODELS OF ARGUMENT},
