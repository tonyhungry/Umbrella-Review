@article{ WOS:000251505900010,
Author = {Lee, Tien-Wen and Dolan, Raymond J. and Critchley, Hugo D.},
Title = {Controlling emotional expression: Behavioral and neural correlates of
   nonimitative emotional responses},
Journal = {CEREBRAL CORTEX},
Year = {2008},
Volume = {18},
