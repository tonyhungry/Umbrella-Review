@article{ WOS:000569549200003,
Author = {Gyurkovics, Mate and Kovacs, Marton and Jaquiery, Matt and Palfi, Bence
   and Dechterenko, Filip and Aczel, Balazs},
Title = {Registered Replication Report of Weissman, D. H., Jiang, J., \& Egner,
   T. (2014). Determinants of congruency sequence effects without learning
   and memory confounds},
