@inproceedings{ WOS:A1997BJ58M00009,
Author = {Barber, P and OLeary, M},
Editor = {Hommel, B and Prinz, W},
Title = {The relevance of salience: Towards an activational account of irrelevant
   stimulus-response compatibility effects},
Booktitle = {THEORETICAL ISSUES IN STIMULUS-RESPONSE COMPATIBILITY},
Series = {ADVANCES IN PSYCHOLOGY},
Year = {1997},
Volume = {118},
Pages = {135-172},
Note = {Symposium on the Theory of S-R Compatibility, BENEDIKTBEUERN, GERMANY,
   JUN 13-15, 1995},
Abstract = {A theoretical analysis of S-R compatibility and the effects of
   irrelevant S-R association is undertaken, assuming that stimulus and
   response attributes are internally represented by nodes that receive and
   communicate activation along temporary task-specific and permanent
   associative pathways. Results of experiments using simple variants of a
   Simon task with the words LEFT and RIGHT as stimuli are used to temper
   the model in the first instance. Other studies with essentially the same
   stimulus material but more complex task demands are used to test the
   model. An independent computational implementation of some key aspects
   of the model by Zorzi and Umilta (1995) adds support to the present
   activational framework for explaining the Simon effect, the effect of an
   irrelevant spatial S-R relationship. Two irrelevant correspondence
   relationships are identified and the strengths of their effects on
   reaction time (RT) are assessed, in addition to that which underlies the
   Simon effect. These are the effects of stimulus congruence (an S-S
   relationship) and word/response-location correspondence (another
   irrelevant S-R relationship). These effects are specific to the stimulus
   material used here but enable questions about the contribution of
   stimulus congruence to performance to be closely examined. The model
   receives strong general support from the present findings, but possible
   modifications and extensions of the model are considered in the light of
   the results.},
