@article{ WOS:000314725500025,
Author = {Sodergren, Mikael and McGregor, Colleen and Farne, Hugo A. and Cao,
   Jiguo and Lv, Zhijun and Purkayastha, Sanjay and Athanasiou, Thanos and
   Darzi, Ara and Paraskeva, Paraskevas},
Title = {A Randomised Comparative Study Evaluating Learning Curves of Novices in
   a Basic Single-Incision Laparoscopic Surgery Task},
Journal = {JOURNAL OF GASTROINTESTINAL SURGERY},
Year = {2013},
Volume = {17},
Number = {3},
Pages = {569-575},
Month = {MAR},
Abstract = {There is currently no objective quantification of the temporal changes
   in performance associated with a novice surgeon learning single-incision
   laparoscopic surgery (SILS) operative tasks. Analysing learning curves
   allows us to objectively quantify performance. The aim was to evaluate
   if the rate of learning and ultimate proficiency level reached in SILS
   when using straight or articulating instruments is different to
   conventional laparoscopy and if training in laparoscopy influences
   learning or proficiency for SILS.
   Thirty-six surgically naive medical students were randomised to complete
   the validated peg transfer task over 50 repetitions using a conventional
   laparoscopic set-up, SILS set-up with straight instruments or
   articulated instruments or SILS set-up after having reached proficiency
   using a conventional laparoscopy.
   There was a significant increased overall proficiency between the group
   trained in conventional laparoscopy and all other groups (p < 0.01),
   with no difference between the other groups. There was no difference in
   the rate of learning between the groups. There was no difference in the
   ultimate proficiency level (p = 0.671) or rate of learning (p = 0.63)
   when using straight or articulating instruments. There was no difference
   in ultimate proficiency level (p = 0.59) or learning rate (p = 0.219)
   seen in the SILS group that had prior training on the task with a
   conventional laparoscopic set-up.
   The results of this study indicate that the proficiency reached using a
   conventional laparoscopic set-up cannot be matched using a SILS
   configuration for the novice surgeon and that the choice of straight or
   articulated instruments as well as previous laparoscopic training does
   not confer an advantage in this basic task.},
