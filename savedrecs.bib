@article{ WOS:000288177300008,
Author = {Rubia, Katya and Cubillo, Ana and Woolley, James and Brammer, Michael J.
   and Smith, Anna},
Title = {Disorder-Specific Dysfunctions in Patients With
   Attention-Deficit/Hyperactivity Disorder Compared to Patients With
   Obsessive-Compulsive Disorder During Interference Inhibition and
   Attention Allocation},
