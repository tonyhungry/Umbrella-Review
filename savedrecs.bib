@article{ WOS:000251438500005,
Author = {Hanoch, Yaniv and Wood, Stacey and Rice, Thomas},
Title = {Bounded rationality, emotions and older adult decision making: Not so
   fast and yet so frugal},
Journal = {HUMAN DEVELOPMENT},
Year = {2007},
Volume = {50},
Number = {6},
Pages = {333-358},
Abstract = {Herbert Simon's work on bounded rationality has had little impact on
   researchers studying older adults' decision making. This omission is
   surprising, as human constraints on computation and memory are
   exacerbated in older adults. The study of older adults' decision-making
   processes could benefit from employing a bounded rationality
   perspective, but any such attempt must take into account the role that
   emotions play in older adults' information processing, memory, and
   attention allocation. This is especially the case because older adults
   show relatively less decrements in performance when tasks are imbedded
   in or laden with emotional stimuli, and they exhibit a greater tendency
   to rely on emotional information. We examine recent work on bounded
   rationality and studies investigating older adults' utilization of, and
   attention to, emotional information, with the aim of creating a
   framework that captures the mechanisms underlying older adults' decision
   making. Copyright (C) 2007 S. Karger AG, Basel.},
