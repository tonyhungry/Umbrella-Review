@article{ WOS:000587405300004,
Author = {Logunov, V, Dmitri and Ponomarev, V, Alexander},
Title = {Karakumosa gen. nov., a new Central Asian genus of fossorial wolf
   spiders (Araneae: Lycosidae: Lycosinae)},
