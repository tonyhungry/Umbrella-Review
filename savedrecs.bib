@article{ WOS:000303932700002,
Author = {Lu, Zhiyong and Kao, Hung-Yu and Wei, Chih-Hsuan and Huang, Minlie and
   Liu, Jingchen and Kuo, Cheng-Ju and Hsu, Chun-Nan and Tsai, Richard
