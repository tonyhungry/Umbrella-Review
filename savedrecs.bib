@article{ WOS:000304104100010,
Author = {Mo, Shenjiang and Booth, Simon A. and Wang, Zhongming},
Title = {How Do Chinese Firms Deal with Inter-Organizational Conflict?},
Journal = {JOURNAL OF BUSINESS ETHICS},
Year = {2012},
Volume = {108},
Number = {1},
Pages = {121-129},
Month = {JUN},
Note = {Memorial Symposium in honor of Professor Bernard Hodgson, Trent Univ,
   Peterborough, CANADA, MAR 25-26, 2010},
Organization = {Trent Ctr Theory, Culture \& Polit; Canadian Res Chair Cultural Studies;
   Trent Cultural Studies Doctoral Program},
Abstract = {Based on social exchange and customer relationship marketing theory,
   this study examines how ethical leadership contributes to
   inter-organizational conflict management (task conflict (TC) and
   relationship conflict), and the moderating role of task interdependence
   in these relationships. Data was collected from 81 suppliers and 45
   corresponding managers of a large group company in China. Results show
   that ethical leadership is negatively associated with the levels of
   inter-organizational conflict, whether task or relationship. Task
   interdependence significantly moderates the relationship between ethical
   leadership and TC. Managerial implication in terms of creating sound
   buyer-supplier relationship through an ethical perspective is discussed.},
