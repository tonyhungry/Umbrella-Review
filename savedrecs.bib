@inproceedings{ WOS:000861612703097,
Author = {Spencer, Jaime and Bowden, Richard and Hadfield, Simon},
Book-Group-Author = {IEEE},
Title = {Medusa: Universal Feature Learning via Attentional Multitasking},
Booktitle = {2022 IEEE/CVF CONFERENCE ON COMPUTER VISION AND PATTERN RECOGNITION
   WORKSHOPS, CVPRW 2022},
Series = {IEEE Computer Society Conference on Computer Vision and Pattern
   Recognition Workshops},
