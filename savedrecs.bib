@article{ WOS:000317723000039,
Author = {Ham, Timothy and Leff, Alex and de Boissezon, Xavier and Joffe, Anna and
   Sharp, David J.},
Title = {Cognitive Control and the Salience Network: An Investigation of Error
   Processing and Effective Connectivity},
