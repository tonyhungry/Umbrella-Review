@article{ WOS:000385576200003,
Author = {Ahmad, M. Faizan and Murphy, James and Vatansever, Deniz and Stamatakis,
   Emmanuel A. and Godsill, Simon J.},
Title = {Bayesian Inference of Task-Based Functional Brain Connectivity Using
   Markov Chain Monte Carlo Methods},
Journal = {IEEE JOURNAL OF SELECTED TOPICS IN SIGNAL PROCESSING},
